{
  "name": "planning-poker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --subresource-integrity",
    "build:prod:gh": "ng build --configuration production --subresource-integrity --output-path docs --base-href=/planning-poker/ && cp docs/index.html docs/404.html ",
    "format:write": "prettier --write \"{src,e2e}/**/*.{js,ts,scss,html}\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": false,
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-case": [
        0,
        "always",
        "lowercase"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format:write",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branch": "master"
  },
  "dependencies": {
    "@angular/animations": "~9.0.0-rc.6",
    "@angular/common": "~9.0.0-rc.6",
    "@angular/compiler": "~9.0.0-rc.6",
    "@angular/core": "~9.0.0-rc.6",
    "@angular/forms": "~9.0.0-rc.6",
    "@angular/platform-browser": "~9.0.0-rc.6",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.6",
    "@angular/pwa": "^0.900.0-rc.6",
    "@angular/router": "~9.0.0-rc.6",
    "@angular/service-worker": "~9.0.0-rc.6",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.0-rc.6",
    "@angular/cli": "~9.0.0-rc.6",
    "@angular/compiler-cli": "~9.0.0-rc.6",
    "@angular/language-service": "~9.0.0-rc.6",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/commit-analyzer": "6.3.3",
    "@semantic-release/exec": "3.3.8",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/npm": "5.3.4",
    "@semantic-release/release-notes-generator": "7.3.5",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "12.6.8",
    "angular-cli-ghpages": "0.5.3",
    "codelyzer": "5.1.0",
    "commitizen": "4.0.3",
    "husky": "3.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "prettier": "1.19.1",
    "protractor": "5.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.20.1",
    "typescript": "~3.6.4"
  }
}
